#include<iostream.h>
class jaffa{
String name;
int c,m,p,s;
void jaff(){
cin>>c,m,p;
s=c+m+s;
}
};
void main(){
int i=0,jk=0,n;
String s[50];
jaffa j[50];
cin>>n;
for(i=0;i<n;i++){
j[i].jaff();
}
for(i=0;i<n;i++){
if((j[i].c>100||j[i].c<0)||(j[i].m>100||j[i].m<0)||(j[i].p>100||j[i].p<0)){
s[j]="Marks of %s is not in range";
j++
}
}
for(i=0;i<n;i++){
if(j[0].s<j.[i]){
j.s[0]=j[i].s;
}
s[j]="%s scored highest";
}
for(i=0;i<j;i++){
cout<<s[i];
}
